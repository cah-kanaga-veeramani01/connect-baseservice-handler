{
  "name": "@service-config/connect-baseservice-handler",
  "version": "1.4.0",
  "description": "Handler for Base Service Configuration",
  "main": "server.ts",
  "publishConfig": {
    "registry": "https://artifacts.cahcommtech.com/repository/commtech-npmjs-internal/"
  },
  "scripts": {
    "build": "rm -rf dist && rm -rf build && tsc && mkdir dist && cp -rf build/ dist/",
    "build:server": "tsc && npm run copy-files",
    "copy-files": "copyfiles --all resources/* ./build",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q build/server.js\"",
    "cache": "jest --clear-cache",
    "start": "node build/server.js",
    "start:dev": "nodemon -e ts -w ./ -x ts-node -r dotenv/config server.ts dotenv_config_path=./env/dev.env",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --watch --detectOpenHandles --logHeapUsage",
    "doc": "typedoc",
    "migrate": "NODE_TLS_REJECT_UNAUTHORIZED='0' flyway -c migration/flyway/flyway.js migrate",
    "info": "flyway -c migration/flyway/flyway.js info",
    "coverage": "node --max-old-space-size=1024 ./node_modules/.bin/jest --coverage --logHeapUsage --runInBand",
    "pact:verify:mock": "node -r dotenv/config build/pacts/verifier.pact.js",
    "test:pact:dev": "node -r dotenv/config build/pacts/verifier.pact.js dotenv_config_path=./env/dev.env"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@pact-foundation/pact": "^9.17.0",
    "@types/config": "^3.3.0",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "ajv": "^8.10.0",
    "axios": "^0.21.4",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-actuator": "^1.8.4",
    "express-http-context": "^1.2.4",
    "express-session": "^1.17.3",
    "helmet": "^4.6.0",
    "js-yaml": "^4.1.0",
    "keycloak-connect": "^19.0.1",
    "locale": "^0.1.0",
    "nanoid": "^3.3.0",
    "node-esapi": "0.0.1",
    "node-flywaydb": "^3.0.7",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pino-pretty": "^9.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.16.1",
    "sequelize-history": "^1.4.0",
    "sequelize-typescript": "^2.1.2",
    "swagger-ui-express": "^4.6.0",
    "ts-node": "^10.9.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express-actuator": "^1.8.3",
    "@types/jest": "^29.2.5",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^18.11.18",
    "@types/sequelize": "^4.28.10",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "concurrently": "^6.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^29.3.1",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "sequelize-mock": "^0.10.2",
    "ts-jest": "^29.0.3",
    "tsc-hooks": "1.1.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": "16.20.1"
  }
}
