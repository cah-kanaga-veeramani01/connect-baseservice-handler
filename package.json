    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "ajv": "^8.10.0",
    "axios": "^0.21.4",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-actuator": "^1.8.4",
    "express-http-context": "^1.2.4",
    "express-session": "^1.17.3",
    "helmet": "^4.6.0",
    "js-yaml": "^4.1.0",
    "keycloak-connect": "^19.0.1",
    "locale": "^0.1.0",
    "nanoid": "^3.3.0",
    "node-esapi": "0.0.1",
    "node-flywaydb": "^3.0.7",
    "pg": "^8.7.1",
