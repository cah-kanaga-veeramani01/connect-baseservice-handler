{
  "name": "@service-configuration/base-service-handler",
  "version": "0.0.11",
  "description": "Handler for Base Service Configuration",
  "main": "server.ts",
  "scripts": {
    "build": "rimraf build && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q build/server.js\"",
    "cache": "jest --clear-cache",
    "start": "node -r dotenv/config build/server.js",
    "start:dev": "nodemon -e ts -w ./ -x ts-node -r dotenv/config server.ts dotenv_config_path=./env/dev.env",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --watch --detectOpenHandles --logHeapUsage",
    "doc": "typedoc",
    "migrate": "NODE_TLS_REJECT_UNAUTHORIZED='0' flyway -c migration/flyway/flyway.js migrate",
    "info": "flyway -c migration/flyway/flyway.js info",
    "coverage": "node --max-old-space-size=1024 ./node_modules/.bin/jest --coverage --logHeapUsage --runInBand"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@types/body-parser": "^1.19.1",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "ajv": "^8.8.2",
    "axios": "^0.21.4",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "helmet": "^4.6.0",
    "js-yaml": "^4.1.0",
    "locale": "^0.1.0",
    "nanoid": "^3.3.0",
    "node-esapi": "0.0.1",
    "node-flywaydb": "^3.0.7",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.16.1",
    "sequelize-history": "^1.4.0",
    "sequelize-typescript": "^2.1.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/config": "0.0.39",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.0.1",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^14.17.14",
    "@types/sequelize": "^4.28.10",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^27.1.0",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "sequelize-mock": "^0.10.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.2"
  }
}
